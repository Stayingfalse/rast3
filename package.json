{
  "name": "random-acts-of-santa-2025",
  "version": "0.1.5",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "docker:build": "docker build -t rao-santa-app --build-arg NEXT_PUBLIC_CLIENTVAR=clientvar .",
    "docker:build:cached": "docker build -t rao-santa-app --build-arg NEXT_PUBLIC_CLIENTVAR=clientvar --cache-from rao-santa-app .",
    "docker:run": "docker run -p 3000:3000 rao-santa-app",
    "docker:run:dev": "docker run -p 3001:3000 rao-santa-app",
    "docker:up": "docker-compose up --build",
    "docker:up:detached": "docker-compose up --build -d",
    "docker:up:cached": "cross-env DOCKER_BUILDKIT=1 docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "analyze": "node scripts/build-analyzer.js",
    "optimize": "npm run analyze && npm run build:analyze"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@aws-sdk/client-s3": "^3.821.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.8.2",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.79.0",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "browser-image-compression": "^2.0.2",
    "date-fns": "^4.1.0",
    "mysql2": "^3.14.1",
    "next": "^15.2.3",
    "next-auth": "^5.0.0-beta.28",
    "nodemailer": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.2",
    "superjson": "^2.2.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.3.3",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.8.2",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "npm@10.9.2",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
